   "source": [
    "# Code here\n",
    "import matplotlib.pyplot as plt\n",
    "import random\n",
    "from numpy import unravel_index\n",
    "%matplotlib inline\n",
    "def accuracy_plot(split_values, leaf_values, X_test, y_test, X_train, y_train):\n",
    "    acc_tst_gp = np.empty((10, 10))\n",
    "    for i in range(10):\n",
    "        for j in range(10):\n",
    "            clf = DecisionTreeClassifier(min_samples_split = split_values[i], min_samples_leaf = leaf_values[j], \n",
    "                                         criterion='entropy')\n",
    "            clf.fit(X_train, y_train)\n",
    "            acc_tst_gp[i][j] = clf.score(X_test, y_test)\n",
    "    max_id = unravel_index(acc_tst_gp.argmax(), acc_tst_gp.shape)\n",
    "    print('The maximum accuracy point is reached when the split size is %s, and the leaf size is %s. The accuracy rate is %s'% \n",
    "          (str(min_samples_split_values[max_id[0]]), str(min_samples_leaf_values[max_id[1]]), str(acc_tst_gp[max_id])))\n",
    "            \n",
    "    for idx in range(10):\n",
    "        plt.plot(split_values, acc_tst_gp[:][idx], label = 'leaf size: '+ str(leaf_values[idx]))\n",
    "    plt.xlabel('tree split size')\n",
    "    plt.ylabel('test accuracy')\n",
    "    plt.title('Decision Tree Classifier')\n",
    "    plt.legend(loc=\"lower right\")\n",
    "    \n",
    "    return plt\n",
    "\n",
    "plt.figure(figsize = (12,8))\n",
    "ax = plt.subplot(111)\n",
    "\n",
    "accuracy_plot(min_samples_split_values, min_samples_leaf_values, X_test, y_test, X_train, y_train)\n",
    "box = ax.get_position()\n",
    "ax.set_position([box.x0, box.y0 + box.height * 0.0 , box.width, box.height * 1])\n",
    "ax.legend(loc = 'upper center', bbox_to_anchor = (0.5, -0.15), fancybox = True, \n",
    "              shadow = True, ncol = 4, prop = {'size':10})"
   ]
  },
