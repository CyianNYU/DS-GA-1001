    "import matplotlib.pyplot as plt\n",
    "import operator\n",
    "%matplotlib inline\n",
    "y_train = train_df.loc[:,11]\n",
    "X_train = train_df.loc[: , 0:10]\n",
    "clf_entro.fit(X_train, y_train)\n",
    "feature = clf_entro.feature_importances_\n",
    "columns_names = [\"revenue\", \"outcalls\", \"incalls\", \"months\", \"eqpdays\", \"webcap\", \n",
    "                 \"marryyes\", \"travel\", \"pcown\", \"creditod\", \"retcalls\"]\n",
    "col_dict = dict(zip(columns_names, feature))\n",
    "indices = dict(sorted(col_dict.items(), key=operator.itemgetter(1), reverse = True))\n",
    "\n",
    "plt.figure(figsize = (12, 8))\n",
    "plt.ylabel('variables')\n",
    "plt.title('Feature Plot')\n",
    "plt.bar(range(len(indices)), indices.values(), align = 'center') \n",
    "plt.xticks(range(len(indices)), indices.keys())\n",
    "plt.show()\n",
